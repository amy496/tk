


var names = ['Wesley Snipes', 'Nicholas Cage', 'Nasir Jones', 'Sean Carter', 'Sean Combs', 'Michael Jordan', 'Patrick Ewing']
let splitnames = []
for (var i = 0; i < names.length; i++){
splitnames.push(names[i].split(" "))
}


let firstNames = []
let lastNames = []

for (var j = 0; j < splitnames.length; j++){
firstNames.push(splitnames[j][0])
   lastNames.push(splitnames[j][1])
}

console.log('firstNames', firstNames)
console.log('lastNames', lastNames)





